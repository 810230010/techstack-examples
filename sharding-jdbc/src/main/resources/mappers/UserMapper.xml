<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.usher.sharding.mapper.UserMapper">

    <select id="findByUserId" resultType="com.usher.sharding.entity.User">
        select
        *
        from t_user
        where
        user_id = #{userId}
    </select>
</mapper>
